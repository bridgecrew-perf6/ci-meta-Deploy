A;;AAAA;AAKA,gBAAA,QALA;AAMA,gBAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,YAAA;AACA,gBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;;AACA,gBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,OAAA,EAAA;AAAA,kBAAA,QAAA,EAAA,KAAA,CAAA,KAAA;AAAA,kBAAA,KAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAAA,iBAAA;;AARA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,OAAA,CAAA,GAAA;AACA,gBAAA,KAAA,CAAA,KAAA,GAAA,IAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA;AAhBA,GAZA;AA8BA,EAAA,OA9BA,qBA8BA;AAAA;;AACA,IAAA,UAAA,CAAA;AAAA,aAAA,MAAA,CAAA,OAAA,GAAA,IAAA;AAAA,KAAA,EAAA,KAAA,CAAA;AACA;AAhCA,CAAA","sourcesContent":["<template>\n  <div>\n    <form style=\"margin: 10px 10px 10px 10px\">\n      <div v-if=\"!this.$store.state.logged\" id=\"hide\">\n        <div class=\"grid-container\">\n          <h3>Log In</h3>\n          <div class=\"grid-x grid-padding-x\">\n            <div class=\"medium-6 cell\">\n              <label>\n                Username\n                <input\n                  v-model=\"uname\"\n                  type=\"text\"\n                  class=\"form-control form-control-lg\"\n                />\n              </label>\n            </div>\n            <div class=\"medium-6 cell\">\n              <label>\n                Password\n                <input\n                  v-model=\"pw\"\n                  type=\"password\"\n                  class=\"form-control form-control-lg\"\n                />\n              </label>\n            </div>\n          </div>\n          <div v-show=\"error\" style=\"margin: 0px 10px 10px 0px; width: 100%;\">\n            <span class=\"label alert expanded\" style=\"display:table; margin:0 auto;\">\n              <i class=\"fi-x-circle\"></i>Error. Please retry\n            </span>\n          </div>\n          <button\n            v-on:click.prevent=\"login()\"\n            type=\"submit\"\n            class=\"submit success button expanded\"\n          >\n            Submit\n          </button>\n        </div>\n      </div>\n      <div v-if=\"this.$store.state.logged && !timeout\" class=\"text-center\"\n        style=\"margin: 10px 10px 10px 10px\">\n        <h2>Welcome {{this.$store.state.logged}}. Logged</h2>\n      </div>\n    </form>\n  </div>\n</template>\n\n<script>\n// import axios from 'axios';\n/* eslint-disable */\nexport default {\n  name: 'Login',\n  data() {\n    return {\n      logged: false,\n      error: false,\n      uname: '',\n      pw: '',\n      posts: [],\n      timeout: false\n    };\n  },\n  methods: {\n    async login() {\n      try {\n        this.error = false;\n        const data = { username: this.uname, password: this.pw, provider: 'db' };\n        const headers = { 'Content-Type': 'application/json' };\n        const response = await this.$http.post('http://51.182.126.80:5000/api/v1/security/login', data, headers);\n        console.log(response.data.access_token);\n        this.logged = true;\n        this.$store.commit('login', {username: this.uname, token: response.data.access_token});\n      }\n      catch (e) {\n        console.log(e);\n        this.error = true;\n      }\n      \n    },\n  },\n  created() {\n            setTimeout(() => this.timeout = true, 10000)\n        }\n};\n</script>\n"],"sourceRoot":"src/components"}]}